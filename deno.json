{
  "imports": {
    "hono": "https://deno.land/x/hono@v3.3.4/mod.ts",
    "hono:middleware": "https://deno.land/x/hono@v3.3.4/middleware.ts",
    "jose": "https://deno.land/x/jose@v4.14.5/index.ts",
    "dotenv": "https://deno.land/std@0.199.0/dotenv/load.ts",
    "mongoose": "npm:mongoose",
    "bcrypt": "https://deno.land/x/bcrypt@v0.3.0/mod.ts"
  },
  "tasks": {
    "dev:app": "deno task dev:ui & deno task dev:api",
    "dev:api": "deno run --allow-net --allow-env --allow-read --allow-sys --watch index.ts",
    "dev:ui": "cd ui && deno task dev",
    "build:ui": "cd ui && deno task build"
  }
}
